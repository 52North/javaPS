<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
                           http://www.springframework.org/schema/util
                           http://www.springframework.org/schema/util/spring-util-4.1.xsd">

    
    <bean class="org.n52.iceland.service.operator.GenericServiceOperator">
        <constructor-arg name="service" value="WPS" />
        <constructor-arg name="version" value="2.0.0" />
    </bean>
    
    <bean class="org.n52.javaPS.operator.GetCapabilitiesOperator" />
    <bean class="org.n52.javaPS.operator.DemoOperator" />

    <bean class="org.n52.javaPS.handler.impl.DemoHandlerImpl" />
    <bean class="org.n52.javaPS.handler.impl.GetCapabilitiesHandlerImpl" />

</beans>
